<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>Stock data graph</title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
        <link rel="stylesheet" href="http://cdn.pydata.org/bokeh/release/bokeh-0.12.0.min.css" type="text/css" />
        <script type="text/javascript" src="http://cdn.pydata.org/bokeh/release/bokeh-0.12.0.min.js"></script>
		{{ script | safe }}
	</head>
	<body>
		<div class=page>
<form  action="" method="post" role="form">
    {{ form.csrf }}
    <div class="form-group">
      <label for="stock_name">stock_name:</label>
      <input type="text" class="form-control" id="stock_name" name="stock_name" placeholder="Stock name?">
      <br>

    </div>
    <button type="submit" class="btn btn-success">Submit</button>
  </form>

 <br>
        {% with messages = get_flashed_messages(with_categories=true) %}
            {% if messages %}

        {% for message in messages %}
            {% if "Error" not in message[1]: %}
                <div class="alert alert-info">
                <strong>Success! </strong> {{ message[1] }}
                </div>
            {% endif %}

            {% if "Error" in message[1]: %}
                <div class="alert alert-warning">
                {{ message[1] }}
                </div>
            {% endif %}
        {% endfor %}
            {% endif %}
        {% endwith %}

</div>
<br>
</div>

  			<h1>
  				Beta Results <br>
  				<a href="/index">Back</a>
  			</h1>
  			{{ div | safe }}
		</div>
	</body>
</html>
